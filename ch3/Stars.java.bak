class Stars 
{
	public static void main(String[] args) 
	{
		char[][] stars1=new char[7][];
		int i=0;
		for(i=0;i<stars1.length;i++){
			stars1[i]=new char[13-i];
			int j=0;
			for(j=0;j<i;j++){
				stars1[i][j]=' ';
				System.out.print(stars1[i][j]);
			}
			int k=0;
			for(k=j;k<stars1[i].length;k++){
				stars1[i][k]='*';
				System.out.print(stars1[i][k]);
			}
			System.out.println('\n');
		}
		char[][] stars2=new char[6][];
		int a=0;
		for(a=0;a<stars2.length;a++){
			stars2[a]=new char[8+a];
			int b=0;
			for(b=0;b<stars2.length-1-a;b++){
				stars2[a][b]=' ';
				System.out.print(stars2[a][b]);
			}
			int c=0;
			for(c=b;c<stars2[a].length;c++){
				stars2[a][c]='*';
				System.out.print(stars2[a][c]);
			}
			//System.out.println('\n');
		}

	}
}
